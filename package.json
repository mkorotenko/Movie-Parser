{
  "name": "common-site",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "start-admin": "ng serve --proxy-config proxy.conf.json --project admin-site",
    "clean_lock": "rimraf mongo-db/mongod.lock",
    "mongo_server": "md mongo-db & \"D:/Program Files/MongoDB/Server/3.2/bin/mongod.exe\" --dbpath D:/server_root/mongo-db --storageEngine=mmapv1",
    "mongo": "npm run clean_lock && npm run mongo_server",
    "server": "node api/dist-server",
    "start1": "concurrently \"npm:mongo\" \"sleep 20\" \"npm:server\"",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.1.1",
    "@angular/cdk": "~7.1.1",
    "@angular/common": "^7.1.1",
    "@angular/compiler": "^7.1.1",
    "@angular/core": "^7.1.1",
    "@angular/forms": "^7.1.1",
    "@angular/http": "^7.1.1",
    "@angular/material": "^7.1.1",
    "@angular/platform-browser": "^7.1.1",
    "@angular/platform-browser-dynamic": "^7.1.1",
    "@angular/router": "^7.1.1",
    "@types/socket.io-client": "^1.4.32",
    "concurrently": "^4.0.1",
    "copy-dir": "^0.4.0",
    "core-js": "^2.5.4",
    "fs-extra": "^7.0.1",
    "get-urls": "^8.0.0",
    "hammerjs": "^2.0.8",
    "iconv-lite": "^0.4.24",
    "jsdom": "^13.0.0",
    "mongodb": "^3.1.8",
    "ngx-monaco-editor": "^6.0.0",
    "parse5": "^5.1.0",
    "ps-tree": "^1.1.0",
    "request": "^2.88.0",
    "rxjs": "^6.0.0",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1",
    "tslib": "^1.9.0",
